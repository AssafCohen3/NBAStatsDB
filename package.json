{
  "name": "vue-electron-flask",
  "version": "1.0.1",
  "private": true,
  "description": "Simple template for combining Electron with Vue3 and Flask",
  "author": "Sanjay Soundarajan",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron:build": "vue-cli-service electron:build",
    "electron:build-preclean": "node ./scripts/prebuild.js",
    "electron:build-release": "vue-cli-service electron:build -p always",
    "electron:serve": "vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "python:build": "python3 -m PyInstaller --onefile --clean ./api.py --distpath ./src/pyflaskdist",
    "python:dev": "python3 ./api.py"
  },
  "main": "background.js",
  "dependencies": {
    "@electron/remote": "^2.0.0",
    "@mdi/font": "5.9.55",
    "axios": "^0.21.2",
    "axios-retry": "^3.2.4",
    "core-js": "^3.6.5",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.6.5",
    "ps-tree": "^1.2.0",
    "roboto-fontface": "*",
    "semver": "^7.3.5",
    "vue": "^3.0.0",
    "vue-i18n": "^9.1.0",
    "vue-router": "^4.0.3",
    "vuetify": "^3.0.0-beta.5",
    "vuex": "^4.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@intlify/vue-i18n-loader": "^3.0.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.1.0",
    "copyfiles": "^2.4.1",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.13",
    "electron-devtools-installer": "^3.1.0",
    "electron-notarize": "^1.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "node-sass": "^7.0.1",
    "postcss": "^8",
    "prettier": "^2.3.2",
    "sass-loader": "^13.0.2",
    "tailwindcss": "^2.2.19",
    "tailwindcss-rtl": "^0.9.0",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-tailwindcss": "~0.1.1",
    "vue-cli-plugin-vuetify": "~2.5.1",
    "webpack-plugin-vuetify": "^2.0.0-alpha.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": "^16.13.0"
  },
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/megasanjay/electron-vue3-flask.git"
  }
}
